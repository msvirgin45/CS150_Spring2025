    "# Print the odd numbers from 0 to 100, and count the total along the way\n",
    "mycounter = 0\n",
    "mynumber = 1\n",
    "while mynumber <= 100:\n",
    "    if mynumber%2 == 1:\n",
    "        print(mynumber, end = ' ')\n",
    "        mycounter = mycounter + 1\n",
    "    mynumber = mynumber + 1\n",
    "print('Total of ' +str(mycounter) + ' odd numbers.')\n"
   ]
