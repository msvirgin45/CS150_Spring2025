    "listind = 0\n",
    "while (listind < len(commands)):\n",
    "    if commands[listind] == 'stop':\n",
    "        totalstops = totalstops + 1\n",
    "    listind = listind + 1\n",
    "print(totalstops)"
   ]
  },
