   ],
   "source": [
    "mylist1 = ['a', 'b', 'c', 'd', 'e']\n",
    "mylist2 = [1, 2, 3, 4, 5]\n",
    "index = 0\n",
    "while index < len(mylist2):\n",
    "    mylist1.append(mylist2[index])\n",
    "    index = index + 1\n",
    "print(mylist1)"
   ]
